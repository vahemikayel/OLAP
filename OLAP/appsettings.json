{
  "ConnectionStrings": {
    "MainConnectionString": ""
  },

  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",

  "Serilog": {
    "MinimumLevel": {
      "Default": "Debug",
      "Override": {
        "Microsoft": "Debug",
        "CatalogAPI": "Debug",
        "MYSHOP": "Debug",
        "System": "Warning"
      }
    },
    "SeqServerUrl": ""
  },

  "Identity": {
    "JWT": {
      "Secret": ""
    },

    "Certificate": {
      "CertificatePath": "",
      "CertificatePassword": "",
      "SigningAlgorithm": ""
    },

    "Password": {
      "RequireDigit": "True",
      "RequireLowercase": "True",
      "RequireUppercase": "True",
      "RequireNonAlphanumeric": "True",
      "RequiredLength": "4",
      "RequiredUniqueChars": "1"
    },

    "Lockout": {
      "DefaultLockoutTimeSpanDays": "1",
      "MaxFailedAccessAttempts": "3"
    },

    "IdentityServer": {
      "InputLengthRestrictions": {
        "Scope": "",
        "Jwt": "",
        "TokenHandle": ""
      }
    }
  }
}
